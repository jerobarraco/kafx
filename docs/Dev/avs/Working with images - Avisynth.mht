table.prettytable th {
  background: #f2f2f2;
  text-align: center;
}

table.wikitable caption,
table.prettytable caption {
  margin-left: inherit;
  margin-right: inherit;
  font-weight: bold;
}

.float-left { float: left; clear: left; }
.float-right { float: right; clear: right; }
.centered { margin-left: auto; margin-right: auto; }

