/* Edited with EditCSS */
/**** LINK-tag style sheet screen.css ****/

/*  screen.css - MoinMoin Default Styles

   Copyright (c) 2001, 2002, 2003 by Juergen Hermann
   */

/* content styles */

body {
    margin:0;
    background: #e5e5e5;
    height: 100%;
}

.footnotes div {
    width: 5em;
    border-top: 1px solid #e5e5e5;
}

input {
    font-size: 1em;
    font-family: Arial, Lucida Grande, sans-serif;
}

textarea {
    font-size: 12px;
    font-family: monospace;
}

span.sep {
    display: none;
}

/* user interface styles */

#header {
    background: #e5e5e5;
    font: 1em, sans-serif;
    margin: 0px 0px 0px 100px;
    padding: 20px 0;
}

#logo {
    margin: 0;
    padding: 5px 10px;
    /* For text only logo */
    font-size: 24px;
    line-height: 1em;
    white-space: nowrap;
}

#logo img {
    vertical-align: middle;
}

#logo a {
    color: #616161;  
    text-decoration: none;
}

#searchform {
    float: right;
    margin: 5px 90px 0 10px;
    padding: 0;
    white-space: nowrap;
}

#searchform form div {
    display: inline;
}

#page {
    margin: 0px 280px 10px 100px;
    padding: 15px 30px;
    border-bottom: 2px solid #9c9c9c;
    background-color: white;
    font:  14px, sans-serif;
    border-top: 1px solid #616161; 

}

#title h1 {
    margin: 0;
    padding:10px 0;
    font-size: 2em;
    line-height: 1.2em;
}

#title h1 a {
    text-decoration: none;
}

#sidebar {
    width: 200px;
    padding: 0;	
    font: bold 
    0.84em,sans-serif;
    float: right;
    clear: right;
    background:#e5e5e5;
    margin: 0px 75px 0px 80px;
}

.sidepanel {
    margin: 0 0 30px 0;
    float: right;
    width: 200px;
    line-height: 1em;
    background: #e5e5e5;
}

.sidepanel h1 {
    margin: 0;
    padding: 0.2em 10px;
    border: none;
    font-size: 1em;
    text-align:left;
    font:  1em,sans-serif; #616161;
    border-top: 1px solid #9c9c9c;
    color: #616161;
    background: #e5e5e5;
}

.sidepanel h2 {
    margin: 0;
    padding: 0.2em 10px;
    font-size: 1em;
    border: none;
    color: #336699;
}

.sidepanel ul {
    list-style-type: none;
    margin: 0;
    padding:0;
}

.sidepanel li {
    margin: 0;
    padding:2px 10px;
    background: #e5e5e5;
}

.sidepanel li form {
    margin: 0;
}

.sidepanel a {
    text-decoration: none;
    display: block;
}

a {
    text-decoration: none;
    color: #336699;
}
a.nonexistent {
/*     text-decoration: underline; */
    color: maroon;
}

/* a:link { */
/*     color: #336699; */
/* } */

/* a:visited { */
/*     color: #336699; */
/* } */

/* a:active { */
/*     color: #336699; */
/* } */


/* a:hover { */
/*     color: #000000; */
/*     text-decoration: underline; */
/* } */

#navibar li.current {
    border: none;
    background: #e5e5e5;
}

.editbar form, .editbar form div {
    display: inline;
    margin: 0;
}

#credits, #version, #timings{
    float: right;
    clear: right;
    margin: 5px 0;
    width: 200px;
    padding: 0;
    list-style-type: none;
    text-align: center;
    font-size: 0.88em;
    color: gray;
}


#message {
    margin: 10px 280px 10px 100px;
    padding: 5px 10px;
    background-color: #F0ECE6;
    border: 1px solid #9c9c9c;
    border-bottom: 2px solid #9c9c9c;
}

#message p{
    margin: 5px 0;
    padding: 0;
    font-weight: bold;
}

#message div.buttons {
    font-weight: normal;
}

form.dialog {
    margin: 0 15px;
}

.dialog td {
    border: none;
    padding: 5px;
}

.dialog td.label {
    text-align: right;
    font-weight: bold;
    width: 25%;
}

*[dir="rtl"] .dialog td.label {
    text-align: left;
}

.dialog td.content input {
    width: 100%;
}

/* We use here dumb css1 ids becuase of IE suckiness */
#editor-textarea, #editor-comment {
    width: 110%;
}

#pagebottom {
    clear: both;
}

#preview {
    border: 2px solid #e5e5e5;
    padding: .5em;
    background: url(../img/draft.png);
}

.diff {
    width:99%;
}

.diff-title {
    background-color: #C0C0C0;
}

.diff-added {
    background-color: #E0FFE0;
    vertical-align: sub;
}

.diff-removed {
    background-color: #FFFFE0;
    vertical-align: sub;
}

.diff-added span {
    background-color: #80FF80;
}

.diff-removed span {
    background-color: #FFFF80;
}

.searchresult dd span {
    font-weight: bold;
}



earchresult dd span {
    font-weight: bold;
}




