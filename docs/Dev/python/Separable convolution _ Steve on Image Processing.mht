  width:50%;
}
table#mainbody table table p {
  width:300px !important;
  overflow:hidden;
}
table#mainbody table div {
margin:0;
padding:0;
  width:650px;
  overflow:hidden;
}

p {
  margin-top:0px;
}
form {
  margin-top:0px;
}

body.cmnty li a:link {
  text-decoration: none;
}
body.cmnty li a:visited {
  text-decoration: none;
}
