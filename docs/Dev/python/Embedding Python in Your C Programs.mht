Content-Disposition: inline; filename=home.css
Content-Type: text/css; charset=utf-8; name=home.css
Content-Location: http://www.linuxjournal.com/sites/linuxjournal.com/themes/linuxjournal/css/home.css
Content-Transfer-Encoding: 8bit

/* Carousel basic */
#carousel {
  position: relative;
  height: 235px;
  width: 100%;
  overflow: hidden;
}
#carousel #carousel_articles div.article {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 234px;
  background-position: top right;
  background-repeat: no-repeat;
}
#carousel #carousel_articles div.article.first { z-index: 1; }
#carousel #carousel_articles div.article.light { background-color: #fff; }
#carousel #carousel_articles div.article.dark { background-color: #000; }
#carousel #carousel_articles div.article h3 {
  padding-top: 30px;
  margin: 0px 30% 0.8em 30px;
  font-size: 16pt;
  font-weight: normal;
  line-height: 1.2em;
  color: #c00;
}
#carousel #carousel_articles div.article p {
  padding-bottom: 30px;
  margin: 0 0 0 30px;
  width: 60%;
  font-size: 11pt;
  line-height: 1.4em;
}
#carousel #carousel_articles div.article.dark h3 { color: #fff; }
#carousel #carousel_articles div.article.dark p { color: #999; }
#carousel #carousel_articles div.article.dark a { color: #399; }

#carousel #carousel_buttons {
  width: 100%;
  position: absolute;
  z-index: 2;
  bottom: 0px;
  background: transparent url("../img/carousel_buttonsbg.gif") repeat-x bottom left;
}
#carousel #carousel_buttons ul {
  margin: 0 29px 0 30px;
  padding: 0;
  list-style: none;
  height: 48px;
}
#carousel #carousel_buttons li {
  float: left;
  width: 25%;
  height: 40px;
  padding-top: 8px;
}
#carousel #carousel_buttons li.active {
  background: transparent url("../img/carousel_buttonsitembg.png") no-repeat top left;
}
#carousel #carousel_buttons a {
  display: block;
  height: 36px;
  margin: 0;
  padding: 2px 5px;
  border-left: 1px solid #999;
  color: #fff;
  text-decoration: none;
  /* rgba is CSS3 only so may cause errors on some browsers */
  text-shadow: 0 1px 0 rgba(0,0,0,0.25);
  overflow: hidden;
}
#carousel #carousel_buttons li.active a,
#carousel #carousel_buttons a:hover { background-color: #c00; }
#carousel #carousel_buttons li.last a { border-right: 1px solid #999; }

/* Content */
body.home #content_body h2 { display: none; }

body.home #content_body table.twocolumn {
  margin: 0;
  padding: 0;
  border: 0;
}
body.home #content_body table.twocolumn tbody { border: 0; }
body.home #content_body table.twocolumn td {
  width: 50%;
  margin: 0;
  padding: 0;
  vertical-align: top;
}
body.home #content_body table.twocolumn div.node.odd { margin-right: 8px; margin-left: 8px; }
body.home #content_body table.twocolumn div.node.even { margin-left: 8px; margin-right: 8px; }

body.home #content_body div.node div.content p a.showfull img,
body.home #content_body div.node div.content p a.showtruncated img {
  vertical-align: baseline;
}

