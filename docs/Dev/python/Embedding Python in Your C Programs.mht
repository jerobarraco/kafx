Content-Disposition: inline; filename=base.css
Content-Type: text/css; charset=utf-8; name=base.css
Content-Location: http://www.linuxjournal.com/sites/linuxjournal.com/themes/linuxjournal/css/base.css
Content-Transfer-Encoding: 8bit

/* -- Base Layout --------------------------------------------------------- */
body {
  margin: 0;
  background: #fff;
  font: 9pt/1.5em Helvetica, Arial, Verdana, sans-serif;
  color: #444;
  width: 100%;
  min-width: 960px;
}

/* Base classes */
a { text-decoration: none; }
a:hover { text-decoration: underline; }
.clear {
  clear: both;
  height: 0;
  line-height: 0;
  overflow: hidden;
  visibility: hidden;
}

/*.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.clearfix { display: inline-block; }*/
/* Hide from IE Mac \*/
/*.clearfix { display: block; }*/
/* End hide from IE Mac */

div.adblock { text-align: center; }
div.adblock img {
  display: block;
  border: 0;
}


/* Header */
#header * {
  margin: 0;
  padding: 0;
}
#header h1 { display: inline; }
#header h1 a {
  float: left;
  display: block;
  width: 158px;
  height: 69px;
  margin: 15px 0 15px 30px;
  background-image: url("../img/header_logo.gif");
  text-indent: -1000em;
  overflow: hidden;
  outline: none;
}
#header div.adblock {
  float: right;
  margin: 5px 15px 5px 5px;
}
#header_menubar {
  clear: both;
  padding: 0 13px 0 20px;
  height: 30px;
  background: #000 url("../img/header_menubarbg.gif") repeat-x top left;
  color: #fff;
}
#header_menubar ul li {
  float: left;
  list-style: none;
  list-style-image: none;
}
#header_menubar ul li a {
  display: block;
  padding: 0 10px;
  height: 30px;
  line-height: 30px;
  color: #fff;
  text-shadow: 0 1px 0 #000;
  text-decoration: none;
  font-weight: bold;
}
#header_menubar ul li a:hover { background-color: #c00; }
#header_menubar ul li ul {
  display: none;
  position: absolute;
  z-index: 3;
  border-top: 1px solid #333;
  background-color: #3f3f3f;
  padding: 5px 0;
}
#header_menubar ul li ul li { float: none; }
#header_menubar ul li.hover a { background-color: #c00; }
#header_menubar ul li.hover ul { display: block; }
#header_menubar ul li.hover ul li a {
  float: none;
  padding-right: 20px;
  background-color: transparent;
  line-height: 24px;
  height: 24px;
}
#header_menubar ul li.hover ul li a:hover { background-color: #c00; }
#header_menubar a.button {
  float: right;
  display: block;
  width: 54px;
  height: 30px;
  margin: 0 0 0 11px;
  background: red url("../img/header_loginbtn-r.gif") no-repeat;
  text-indent: -1000em;
  overflow: hidden;
  text-decoration: none;
  outline: none;
}
#header_menubar a.button:active { background-position: 0 -30px; }
#header_menubar form h3 { display: none; }
#header_menubar form {
  float: right;
  margin: 4px 0 0 0;
}
#header_menubar form * { vertical-align: top; }
#header_menubar form input.form-text {
  margin: 4px -5px 0 0;
  background: transparent url("../img/header_searchtextbg.gif") no-repeat center left;
  border: 0;
  padding: 3px 3px 3px 10px;
  width: 127px;
  height: 16px;
  line-height: 16px;
  font-size: 9pt;
}
#header_menubar form input.form-submit {
  background: transparent url("../img/header_searchbtn.gif") no-repeat top left;
  margin-top: 4px;
  border: 0;
  width: 60px;
  height: 22px;
  text-indent: -1000em;
  word-spacing: 10em; /* Resolves IE non indentation */
  overflow: hidden;
  cursor: pointer;
}
#header_menubar form input.form-submit:active { background-position: 0 -22px; }

#header_breadcrumb {
  border-bottom: 1px solid #ccc;
  padding: 3px 30px 1px 30px;
  color: #999;
}
#header_breadcrumb a { color: #999; }
#header_breadcrumb span { margin: 0 5px; }




/* Content */


#content {
  background: transparent url("../img/content_bg.gif") repeat-y top right;
  padding-right: 366px;
}
#content a { color: #c00; }

#content_right {
  position: relative;
  float: left;
  margin-right: -366px;
  width: 366px;
  background: transparent url("../img/content_bg.gif") repeat-y top right;
  padding-bottom: 15px;
}
#content_right h1,
#content_right h2,
#content_right h3 { font-size: 11pt; }

#content_left {
  position: relative;
  float: left;
  width: 100%;
  min-width: 100%; /* Leave it for IE 7 */
}
#content_body { padding: 30px; }
#content_body h2 {
  margin: 0 0 1em 0;
  font-weight: normal;
  font-size: 17pt;
  line-height: 1.3em;
  color: #000;
}

#content div.head h3 a {
  font-size: .8em;
  color: #2f4f4f;
}
#content div.content p a {
  color: #c00;
}
#content_body .messages.error a {
  color: #d3d3d3;
}
/*#content_body div.block h3 {
background: url("../img/arrow.png") no-repeat center left;
text-indent: 26px;
}*/
/*#arrow {
background: url("../img/arrow.png") no-repeat center;
padding-left: 24px;
display: inline;
position: relative;
}*/

#content div.block h3 {
  color: #c00;
  font-size: 1.4em;
  margin-bottom: .5em;
  margin-left: .3em;
  border-bottom: solid 2px #ccc;
}
/* Footer */
#footer {
  clear: both;
  padding: 5px 30px;
  background-color: #000;
  text-align: right;
  color: #444;
}
#footer p { margin: 0; }
#footer ul {
  float: left;
  text-align: left;
  margin: 0;
  padding: 0;
  list-style: none;
}
#footer ul li {
  display: inline;
  margin: 0 0.5em 0 0;
  padding: 0;
}
#footer p.message {
  clear: left;
  text-align: left;
  margin-top: 0.5em;
}
#footer a { color: #666; }
#footer a:hover { color: #fff; }



/* -- Important page objects ---------------------------------------------- */



/* Subscribe + Ad */
#subscribe {
  background: transparent url("../img/subscribe_bg.gif") no-repeat top left;
  color: #066;
  height: 55px;
}
#subscribe h3 { display: none; }
#subscribe ul {
  margin: 0 15px;
  padding: 31px 0 0 0;
  list-style: none;
}
#subscribe ul li {
  display: inline;
  padding: 0;
  margin: 0 5px 0 0;
}
#subscribe ul li a {
  font-size: 8.5pt;
  color: #066;
  /* rgba is CSS3 only so may cause errors on some browsers */
  text-shadow: 0 1px 0 rgba(255,255,255,0.8);
}
#subscribe ul li a:hover { color: #000; }

/* Latest issue */
#latestissue div.header {
  height: 10px;
  line-height: 10px;
/*  background: transparent url("../img/latestissue_headerbg.gif") no-repeat top left;*/
}
#latestissue div.footer {
  height: 10px;
  line-height: 10px;
  /*background: transparent url("../img/latestissue_footerbg.gif") no-repeat top left;*/
}
#latestissue div.content {
/*  background: transparent url("../img/latestissue_contentbg.gif") repeat-y top left;*/
  padding: 10px 0 0 0;
}
#latestissue img {
  margin-left: 10px;
  float: right;
}
#latestissue h3 {
  margin: 0 0 10px 0;
  font-size: 14pt;
  font-weight: normal;
  color: #066;
  text-shadow: 0 1px 0 #fff;
}
#latestissue h4 {
  width: 160px;
  margin: 0px 0px 10px 0;
  text-align: left;
  font-weight: normal;
  font-size: 1em;
  color: #033;
}
#latestissue h4 span {
  padding-left: 40px;
  line-height: 16pt;
  font-size: 14pt;
  color: #699;
}
#latestissue p {
  margin: 0 0 0 0;
  text-align: left;


}
#latestissue p a.button {
  margin: 0;
  
  display: block;
  width: 160px;
  height: 18px;
  line-height: 18px;
  background: transparent url("../img/latestissue_buttonbg.gif") no-repeat top left;
  text-decoration: none;
  outline: none;
}
#latestissue p a.button:link,
#latestissue p a.button:visited { color: #fff; } /* Keep button text white */
#latestissue p a.button:active {
  background-position: 0 -18px;
  color: #9cc;
}
#latestissue form {
  margin: 30px 140px 0 0;
  padding: 0;
}
#latestissue form select {
  margin-top: 10px;
  width: 155px;
}
#latestissue .blurb {
  
  margin-left: 180px;
}
.contentad {
  text-align: center;
}

/* Tabs */
#content_body div.tabs { margin-bottom: 30px; }

/* Messages */
#content_body div.messages {
  padding: 5px 10px;
  margin-bottom: 1em;
  font-size: 12pt !important;
}
#content_body div.messages.status {
  background-color: #690;
  color: #fff;
}
#content_body div.messages.error {
  background-color: #c00;
  color: #fff;
  border: 0 !important;
}

/* Profile */
#content_body div.profile { clear: left; }
#content_body div.profile h2.title {
  font-size: 12pt;
  margin: 1.5em 0 0 0;
}

/* Node head */
#content_body div.node { margin-bottom: 20px; }
#content_body div.node div.head { margin-bottom: 15px; }
#content_body div.node div.head h3 {
  margin: 0;
  font-weight: bold;
  font-size: 12pt;
  line-height: 1.2em;
  border: none;
  background: none;
}
#content_body div.node div.head h4,
#content_body div.node div.head h5,
#content_body div.node div.head h6 {
  font-weight: normal;
  margin: 0.2em 0 0 0;
  color: #999;
  font-size: 10pt;
}
#content_body div.node div.head h4 a { color: #999; }
#content_body div.node div.head h4 a:hover { color: #666; }
#content_body div.node div.head h4 span.taxonomy a {
  text-decoration: none;
/*  background-color: #ffc;*/
 /* border: 1px solid #ddd;*/
  padding: 1px 3px;
}
#content_body div.node div.head h4 span.taxonomy a:hover {
  border-color: #999;
}

/* Node content */
#content_body div.node div.content h2 {
  font-size: 17pt;
  margin: 2em 0 1em 0;
}
#content_body div.node div.content blockquote {
  font-style: italic;
  background: transparent url("../img/node_blockquotebg.gif") no-repeat top left;
  margin: 1em 30px;
  padding-left: 35px;
}

/* Node links */
#content_body div.node div.links { margin-top: 15px; }
#content_body div.node div.links a { color: #2f4f4f; }
#content_body div.node div.links li { padding: 0 1em 0 0; }
#content_body div.node div.links li.blog_usernames_blog a {
  padding-left: 20px;
  background: url("../img/node_blogicon.gif") no-repeat center left;
}
#content_body div.node div.links li.comment_add a,
#content_body div.node div.links li.comment_comments a {
  padding-left: 20px;
  background: url("../img/node_commentsicon.gif") no-repeat center left;
}
#content_body div.node div.links li.node_read_more a {
  padding-left: 20px;
  background: url("../img/node_viewsicon.gif") no-repeat center left; 
}

/* Comments */
#content_body #comments { margin-top: 30px; }
#content_body #comments div.comment {
  margin: 0 -30px;
  padding: 15px 30px;
}
#content_body #comments div.comment.even,
#content_body #comments div.indented div.comment.even {
  border-top: 1px solid #ddd;
